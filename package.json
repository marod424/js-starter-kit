{
  "name": "js-starter-kit",
  "version": "0.1.0",
  "description": "JavaScript development environment starter-kit",
  "main": "index.js",
  "scripts": {
    "prestart": "babel-node build/start-message.js",
    "start": "npm-run-all --parallel security-check start-dev-server lint:watch test:watch",
    "start-dev-server": "babel-node build/dev-server.js",
    "lint": "esw webpack.config.* src build --color",
    "lint:watch": "npm run lint -- --watch",
    "security-check": "nsp check",
    "test": "mocha --reporter progress --colors build/test-setup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marod424/js-starter-kit.git"
  },
  "author": "Matthew Rodriguez",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/marod424/js-starter-kit/issues"
  },
  "homepage": "https://github.com/marod424/js-starter-kit#readme",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-preset-latest": "^6.22.0",
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "css-loader": "^0.26.1",
    "eslint": "^3.16.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-watch": "^3.0.0",
    "express": "^4.14.1",
    "jsdom": "^9.11.0",
    "mocha": "^3.2.0",
    "npm-run-all": "^4.0.1",
    "nsp": "^2.6.2",
    "numeral": "^2.0.4",
    "open": "^0.0.5",
    "style-loader": "^0.13.1",
    "webpack": "1.13.2",
    "webpack-dev-middleware": "^1.10.1"
  }
}
